{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "build": {
      "env": {
        "NODE_VERSION": "16.x"
      },
      "commands": {
        "build": "npm run build"
      }
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }
  